{
    "env": {
        "browser": true,
        "jquery": true,
        "amd": true
    },
    "plugins": [
        "amd-imports"
    ],
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}
